<div class="row">
                <div class="col-md-6 mt-5">
                    <input class="form-control" type="text" placeholder="Ingrese aquí su búsqueda.">
                </div>
                <div class="col-md-4 mt-3 ">
                    <button class="btn btn-primary " id="btn-Search">Search</button>
                </div>
                <div class="col-md-2 mt-5" >
                    <button class="btn btn-danger" id="btn-Post">Post a job</button>
                </div>
            </div>

{{#if activeProgramacion}}
<h2><img src="https://www.pngitem.com/pimgs/m/174-1746684_java-java-logo-black-png-transparent-png.png"
        id="img-programming">Programacion</h2>
{{/if}}

{{#if activeDesing}}
<h2><img src="https://us.123rf.com/450wm/pytyczech/pytyczech1707/pytyczech170700051/81581133-icono-de-gota-negra-s%C3%ADmbolo-de-aceite-o-agua-ejemplo-plano-simple-del-vector-con-la-sombra-aislada-e.jpg?ver=6"
        alt="desing.JPG" id="img-desing">Desing</h2>
{{/if}}

    
<br>
<table class="table table-bordered table-hover mt-4">
    <thead class="table-dark">
        <tr>
            <th scope="col">Location</th>
            <th scope="col">Company</th>
            <th scope="col">Position</th>
        </tr>
    </thead>
    <tbody>
        {{#each datos}}
        <tr class='clickable-row' data-href='/job/{{this.id}}' style="cursor: pointer;">
            <td>{{this.location}}</td>
            <td>{{this.company}}</td>
            <td>{{this.position}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>